{%- import "templates/_helpers_layout.jinja2" as helpers_layout %}
{{ whh.tags.Doctype().html5() }}
<html>
<head>
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
  <meta name="google-site-verification" content="xBVm_AfJYEdA-tbww41dajF9RuGbZHAjpOBhwWwqbcM">
  <title>CCHDO{%- call h.title() %}{%- block subtitle %}{%- endblock %}{%- block title %}{%- endblock %}{%- endcall %}</title>
  <link rel="shortcut icon" href="/static/favicon.ico">
  <link rel="icon" type="image/png" href="/static/favicon.png">
  {{ whh.tags.stylesheet_link('http://yui.yahooapis.com/3.3.0/build/cssreset/reset-min.css') }}
  {{ whh.tags.stylesheet_link('/static/css/screen.css') }}
  {%- if _static %}{{ whh.tags.stylesheet_link('/static/css/static.css') }}{%- endif %}
  {%- include 'templates/_ie_compat.jinja2' ignore missing %}
  {%- block staff_head %}{%- endblock %}
  {%- block head %}{%- endblock %}
  {%- block subhead %}{%- endblock %}
</head>
<body>
  <div id="centering" class="{%- block fullwidth_class %}{%- endblock %}">
    {% include 'templates/_base_header.jinja2' with context %}
    <div id="content"{%- block content_attrs -%}{%- endblock %}>
      {%- if request.session.peek_flash('action_taken') -%}
        {% set actions_taken = request.session.pop_flash('action_taken') %}
        {% for action in actions_taken %}
          <h1 class="flash action_taken">{{ whh.HTML.literal(action) }}</h1>
        {% endfor %}
      {%- endif -%}
      {%- if request.session.peek_flash('help') %}
        {% set help = request.session.pop_flash('help') %}
        {% for helper in help %}
          <h1 class="flash help">{{ whh.HTML.literal(helper) }}</h1>
        {% endfor %}
      {%- endif -%}
      {%- block staff_content %}{%- endblock %}
      {%- block subcontent %}{%- endblock %}
      {% block content -%}
        {% if _static -%}{% include _static %}{%- endif %}
      {%- endblock %}
    </div>
    {% include 'templates/_base_footer.html' %}
  </div>
  <div id="scripts">
    {% block js %}{% endblock %}
    <script>
      (function () {
        function add_class(e, c) {
          e.className = e.className + ' ' + c;
        }
        function remove_class(e, c) {
          e.className = e.className.replace(' ' + c, '');
        }
        function addfocusblur(e, limit) {
          var lis = [];
          var f = e;
          while (f !== limit) {
            if (f.tagName == 'LI') {
              lis.push(f);
            }
            f = f.parentNode;
          }
          e.onfocus = function () {
            for (var i = 0; i < lis.length; i++) {
              add_class(lis[i], 'focus');
            }
          };
          e.onblur = function () {
            for (var i = 0; i < lis.length; i++) {
              remove_class(lis[i], 'focus');
            }
          };
        }
        function walkFor(root, tagname, callback) {
          for (var i = 0; i < root.children.length; i++) {
            walkFor(root.children[i], tagname, callback);
          }
          if (root.tagName == tagname) {
            callback(root);
          }
        }
        var menu = document.getElementById('cchdo_menu');
        walkFor(menu, 'A', function (a) {
          addfocusblur(a, menu);
        });
      })();
    </script>
    {{ whh.tags.javascript_link('/static/js/banner_rotate.js') }}
    {%- include 'templates/_track.html' ignore missing %}
  </div>
</body>
</html>
