{{ whh.tags.Doctype().html5() }}
{%- import "templates/_helpers_layout.jinja2" as helpers_layout %}
<html>
<head>
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
  <meta name="google-site-verification" content="Jg9hBGPowxUZWHJAgYLXRFfZweb_R28puGLp0H6xA3E">
  <title>{%- call h.title() %}{%- block title %}{%- endblock %}{%- block subtitle %}{%- endblock %}{%- endcall %}CCHDO</title>
  <link rel="shortcut icon" href="/static/favicon.ico">
  <link rel="icon" type="image/png" href="/static/favicon.png">
  <link rel="apple-touch-icon-precomposed" type="image/png" href="/static/img/iphone_icon.png">
  {{ whh.tags.stylesheet_link('/static/css/normalize.css') }}
  {{ whh.tags.stylesheet_link('/static/css/screen.css') }}
  {%- include 'templates/_ua.jinja2' ignore missing %}
  {%- block staff_head %}{%- endblock %}
  {%- block head %}{%- endblock %}
  {%- block subhead %}{%- endblock %}

  {{ whh.tags.stylesheet_link('/static/css/handheld.css', media='only screen and (max-device-width:600px)', class="mobile") }}
  {{ whh.tags.stylesheet_link('/static/css/handheld.css', media='screen and (-webkit-device-pixel-ratio:1.5)', class="mobile") }}
  {{ whh.tags.stylesheet_link('/static/css/handheld.css', media='handheld', class="mobile") }}
  <meta class="mobile" name="viewport" content="width=device-width initial-scale=1 user-scalable=1 target-densitydpi=device-dpi">

  {{ whh.tags.stylesheet_link('http://fonts.googleapis.com/css?family=Open+Sans') }}
</head>
<body>
  <div id="centering" class="{%- block fullwidth_class %}{%- endblock %}">
    {%- include 'templates/_base_header.jinja2' with context %}
    <div id="content"{%- block content_attrs -%}{%- endblock %}>
      {%- if request.session.peek_flash('action_taken') -%}
        {%- set actions_taken = request.session.pop_flash('action_taken') %}
        {%- for action in actions_taken %}
          <h1 class="flash action_taken">{{ whh.HTML.literal(action) }}</h1>
        {%- endfor %}
      {%- endif -%}
      {%- if request.session.peek_flash('help') %}
        {%- set help = request.session.pop_flash('help') %}
        {%- for helper in help %}
          <h1 class="flash help">{{ whh.HTML.literal(helper) }}</h1>
        {%- endfor %}
      {%- endif -%}
      {%- block staff_content %}{%- endblock %}
      {%- block subcontent %}{%- endblock %}
      {% block content -%}{%- endblock %}
    </div>
    {%- include 'templates/_base_footer.html' %}
  </div>
  <div id="scripts">
    {%- block js %}{%- endblock %}
    {{- whh.tags.javascript_link('/static/js/base.js') }}
    {{- whh.tags.javascript_link('/static/js/click_tracker.js') }}
    {%- include 'templates/_track.html' ignore missing %}
    {%- include 'templates/_ie6.html' ignore missing %}
  </div>
</body>
</html>
