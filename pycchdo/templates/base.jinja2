{% import "templates/_helpers_layout.jinja2" as helpers_layout -%}
{{ whh.tags.Doctype().html5() }}
<html>
<head>
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
  <meta name="google-site-verification" content="xBVm_AfJYEdA-tbww41dajF9RuGbZHAjpOBhwWwqbcM">
  <title>CCHDO{% call h.title() %}{% block title %}{% endblock %}{% endcall %}</title>
  <link rel="shortcut icon" href="/static/favicon.ico">
  <link rel="icon" type="image/png" href="/static/favicon.png">
  {{ whh.tags.stylesheet_link('http://yui.yahooapis.com/3.3.0/build/cssreset/reset-min.css') }}
  {{ whh.tags.stylesheet_link('/static/css/screen.css') }}
  {% include 'templates/_ie_compat.jinja2' ignore missing %}
  {%- block head -%}{%- endblock -%}
</head>
<body>
  <div id="centering">
    {% include 'templates/_base_header.jinja2' %}
    <div id="content"{%- block content_attrs -%}{%- endblock %}>
      {%- if request.session.peek_flash('action_taken') -%}
        <h1 class="flash action_taken">{{ request.session.pop_flash('action_taken') }}</h1>
      {%- endif -%}
      {% block content %}{% endblock %}
    </div>
    {% include 'templates/_base_footer.html' %}
  </div>
  <div id="scripts">
    {% block js %}{% endblock %}
    {% include 'templates/_track.html' ignore missing %}
    {% include 'templates/_janrain.html' ignore missing %}
  </div>
</body>
</html>
