{%- block content %}
  {%- call h.boxed('%s Cruises' % basin, class='by-ocean %s' % basin.lower()) %}
    <div id="map_side">
      <p>Mouse over the map and click on a highlighted cruise to visit its
        cruise page.
      </p>
      {%- if basin == 'Arctic' or basin == 'Southern' %}
        <p>Please note, the {{ whh.tags.link_to('Project Carina collection', request.route_path('project_carina')) }} is not shown on this map.</p>
      {%- endif %}
      {%- if basin == 'Southern' %}
        <p><a href="/sou_map.html">WOCE Reference Map</a></p>
      {%- endif %}
      <div id="map" class="mapimg">
        <img src="{{h.get_basin_map(basin, None)}}" usemap="{{ h.image_map_id(basin) }}">
        {{ image_map_for(basin) }}
      </div>
    </div>
    <div id="collection_side">
      {%- if areas %}
      <div id="basin_area_selector">
        <p>Jump to an area:</p>
        <ol>
        {%- for area in areas %}
          <li>{{ whh.tags.link_to(area, '#area-%s' % area)}}</li>
        {%- endfor %}
        </ol>
      </div>
      {%- endif %}

      <div id="cruises" class="scrollable-y">
        {%- if collections['default'] %}
          {{ show_area(None, collections['default']) }}
        {%- else %}
          {%- for area in areas %}
            {{ show_area(area, collections[area.lower()]) }}
          {%- endfor %}
        {%- endif %}
      </div>
    </div>
  {%- endcall %}
{%- endblock %}
{%- block js %}
  {{ whh.tags.javascript_link('/static/js/metaTable.js') }}
  {{ whh.tags.javascript_link('/static/js/basin.js') }}
{%- endblock %}
