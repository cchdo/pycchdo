{%- extends "base.jinja2" %}
{%- block head %}
  {{ whh.tags.stylesheet_link('/static/css/metaTable.css') }}
  {{ whh.tags.stylesheet_link('/static/css/basin.css') }}
{%- endblock %}
{%- block title %} Basin | {{basin}}{%- endblock %}
{%- macro show_area(area, collections) %}
  {%- if area %}
    <h2 id="area-{{area}}">{{ area }}</h2>
  {%- endif %}
  <table class="has-meta-bodies pre-expand">
    <tr class="header">
      <th>Collection</th>
      <th>Year</th>
      <th>Ship</th>
      <th>Chief Scientist</th>
    </tr>
    {%- for collection in collections %}
      {%- set hl = loop.cycle('even', 'odd') %}
      {%- set loopi = loop.index0 %}
      {%- set cruises = coll_cruises[collection] %}
      <tr class="meta mb-link{{ loopi }} {{ hl }}">
        <td>{{h.link_collection(collection)}}</td>
        <td colspan="4">
          {{h.lazyload_image(request, h.get_basin_map(basin, collection, cruises), lazyload=False, class_='map')}}
        </td>
      </tr>
      {%- for cruise in cruises %}
        <tr class="body mb-link{{ loopi }} {{ hl }} {{ loop.cycle('even1', 'odd1') }}{%- if not cruise.track %} no_plot{%- endif %}">
          <td>{{h.link_cruise(cruise)}}</td>
          <td>{{cruise.date_start.year}}</td>
          <td>{{h.link_ship(cruise.ship)}}</td>
          <td>
            {%- set css = cruise.chief_scientists %}
            {%- if css %}
              {{h.link_person(css[0].person)}}
            {%- endif %}
          </td>
        </tr>
      {%- endfor %}
    {%- endfor %}
  </table>
{%- endmacro %}
