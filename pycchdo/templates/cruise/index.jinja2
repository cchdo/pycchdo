{% extends "templates/base.jinja2" %}
{% block title %}Cruises{% endblock %}
{% block content %}
  {% call h.boxed('Cruises') -%}
    <ul class="autopagerize_page_element">
    {% for cruise in cruises %}
      <li>{{ h.link_cruise(cruise) }}</li>
    {% endfor %}
    </ul>
    {{ h.pager_for(cruises) }}
    <a href="/objs/new?_obj_type=Cruise">New Cruise</a>
  {%- endcall %}
{% endblock %}

