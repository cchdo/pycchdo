{% extends "templates/base.jinja2" %}
{% block title %}People{% endblock %}
{% block content -%}
  {% call h.boxed('People') -%}
    <ul>
    {%- for person in people %}
      <li>{{ whh.tags.link_to(person.full_name(), '/person/%s' % person.id) }}</li>
    {%- endfor %}
    </ul>
    {{ h.pager_for(people) }}
  {%- endcall %}
{%- endblock %}
