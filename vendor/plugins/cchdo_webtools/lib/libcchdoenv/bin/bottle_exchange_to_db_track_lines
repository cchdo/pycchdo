#!/usr/bin/env python2.6
from __future__ import with_statement
import os; activate_this=os.path.join(os.path.dirname(__file__), 'activate_this.py'); execfile(activate_this, dict(__file__=activate_this)); del os, activate_this

import sys

import libcchdo as L
import libcchdo.formats.common.track_lines as track_lines


def main(argv):
    if len(argv) < 2:
        print 'Usage:', argv[0], '<exchange bottle file>'
        return 1
    
    with open(argv[1], 'r') as in_file:
        file = L.fns.read_arbitrary(in_file)
        track_lines.write(file, sys.stdout)


if __name__ == '__main__':
    sys.exit(main(sys.argv))
